% wint_freq_hc = (mean(wint_gwtc_hc, 1))*100;
% wint_freq_hw = (mean(wint_gwtc_hw, 1))*100;
% wint_freq_s = (mean(wint_gwtc_s, 1))*100;
% wint_freq_w = (mean(wint_gwtc_w, 1))*100;
% wint_freq_wfp = (mean(wint_gwtc_wfp, 1))*100;
% 
% %% FREQUENCY OF OCCURRENCE SPRING
% 
% spr_freq_c = (mean(spr_gwtc_c, 1))*100;
% spr_freq_cfp = (mean(spr_gwtc_cfp, 1))*100;
% spr_freq_d = (mean(spr_gwtc_d, 1))*100;
% spr_freq_dc = (mean(spr_gwtc_dc, 1))*100;
% spr_freq_dw = (mean(spr_gwtc_dw, 1))*100;
% spr_freq_h = (mean(spr_gwtc_h, 1))*100;
% spr_freq_hc = (mean(spr_gwtc_hc, 1))*100;

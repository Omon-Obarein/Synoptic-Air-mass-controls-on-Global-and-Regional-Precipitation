%% FREQUENCY OF OCCURRENCE WINTER
% 
% wint_freq_c = (mean(wint_gwtc_c, 1))*100;
% wint_freq_cfp = (mean(wint_gwtc_cfp, 1))*100;
% wint_freq_d = (mean(wint_gwtc_d, 1))*100;

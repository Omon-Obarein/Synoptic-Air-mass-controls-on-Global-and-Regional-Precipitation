trop_relcon_WFP = relcont(trop_sd, trop_gwtc_wfp, trop_freq_wfp); 
dry_relcon_WFP = relcont(dry_sd, dry_gwtc_wfp, dry_freq_wfp); 
mild_relcon_WFP = relcont(mild_sd, mild_gwtc_wfp, mild_freq_wfp); 
cold_relcon_WFP = relcont(cold_sd, cold_gwtc_wfp, cold_freq_wfp); 
polar_relcon_WFP = relcont(polar_sd, polar_gwtc_wfp, polar_freq_wfp); 
high_relcon_WFP = relcont(high_sd, high_gwtc_wfp, high_freq_wfp); 
hstrop_relcon_WFP = relcont(hstrop_sd, hstrop_gwtc_wfp, hstrop_freq_wfp); 
med_relcon_WFP = relcont(med_sd, med_gwtc_wfp, med_freq_wfp); 
mwc_relcon_WFP = relcont(mwc_sd, mwc_gwtc_wfp, mwc_freq_wfp); 
